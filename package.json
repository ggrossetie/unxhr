{
  "name": "unxhr",
  "description": "Synchronous and asynchronous XMLHttpRequest for Node",
  "version": "1.2.0",
  "type": "module",
  "main": "dist/unxhr.js",
  "module": "dist/unxhr.module.js",
  "source": "lib/index.js",
  "exports": {
    ".": {
      "import": "./dist/unxhr.mjs",
      "require": "./dist/unxhr.js"
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.import.config.js && rollup -c rollup.module.config.js && rollup -c rollup.require.config.js",
    "test": "mocha tests/test-*.js && ts-node node_modules/mocha/lib/cli/cli.js tests/test-request.js",
    "lint": "standard tests/**.js lib/**.js example/**.js",
    "lint-fix": "npm run lint -- --fix"
  },
  "authors": [
    "Dan DeFelippi (http://driverdan.com)",
    "Guillaume Grossetie (https://github.com/mogztter)",
    "David Jencks (https://gitlab.com/djencks)"
  ],
  "keywords": [
    "xhr",
    "ajax",
    "sync",
    "http"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mogztter/unxhr.git"
  },
  "bugs": "http://github.com/mogztter/unxhr/issues",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "dirty-chai": "2.0.1",
    "get-port": "5.1.1",
    "mocha": "9.1.3",
    "rollup": "^2.67.0",
    "standard": "16.0.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
